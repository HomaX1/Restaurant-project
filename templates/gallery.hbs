<div class="container gallery-wrap">
    <h3 class="gallery-wrap__title title title_margin">Gallery</h3>
    <div class="gallery-products">
        {{#each menu}}
            <div class="gallery-product">
                <img class="gallery-product__img" src="{{imgBig}}" alt="Food">
            </div>
        {{/each}}
    </div>
</div>
<div class="modal-wrap">
    <div class="modal-product">
        <i class="modal-product__icon-cancel fa fa-times" aria-hidden="true"></i>
        <img class="modal-product__img" src="" alt="Food">
    </div>
</div>